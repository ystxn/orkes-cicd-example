{
  "createTime": 1721376582130,
  "updateTime": 1724988246208,
  "name": "news-data-indexer",
  "description": "Index news data",
  "version": 2,
  "tasks": [
    {
      "name": "looper",
      "taskReferenceName": "looper_ref",
      "inputParameters": {
        "loops": 30
      },
      "type": "DO_WHILE",
      "decisionCases": {},
      "defaultCase": [],
      "forkTasks": [],
      "startDelay": 0,
      "joinOn": [],
      "optional": false,
      "defaultExclusiveJoinTask": [],
      "asyncComplete": false,
      "loopCondition": "(function () {\n  return $.looper_ref['iteration'] < $.loops;\n})();",
      "loopOver": [
        {
          "name": "get-news",
          "taskReferenceName": "get-news_ref",
          "inputParameters": {
            "uri": "https://financialmodelingprep.com/api/v3/fmp/articles?page=${looper_ref.output.iteration}&size=20&apikey=${workflow.secrets.fmp-key}",
            "method": "GET",
            "connectionTimeOut": 3000,
            "readTimeOut": "3000",
            "accept": "application/json",
            "contentType": "application/json"
          },
          "type": "HTTP",
          "decisionCases": {},
          "defaultCase": [],
          "forkTasks": [],
          "startDelay": 0,
          "joinOn": [],
          "optional": false,
          "defaultExclusiveJoinTask": [],
          "asyncComplete": false,
          "loopOver": [],
          "onStateChange": {},
          "permissive": false
        },
        {
          "name": "transform-to-dynamic",
          "taskReferenceName": "transform-to-dynamic_ref",
          "inputParameters": {
            "expression": "const sanitise = (input) => input\n  .replace(/<[^>]*>?/gm, '')\n  .replace(/\\n/g, ' ')\n  .substr(0, 1535);\n\n(() => $.data.map((entry) => ({\n  \"embeddingModelProvider\": $.modelProvider,\n  \"embeddingModel\": $.model,\n  \"vectorDB\": $.vectorDB,\n  \"index\": $.index,\n  \"namespace\": $.index,\n  \"text\": sanitise(entry.content),\n  \"docId\": entry.title.replace(/[^a-zA-Z0-9]/g, '')\n})))();",
            "evaluatorType": "graaljs",
            "data": "${get-news_ref.output.response.body.content}",
            "vectorDB": "pinecone",
            "index": "news-small",
            "modelProvider": "openai",
            "model": "text-embedding-3-small"
          },
          "type": "INLINE",
          "decisionCases": {},
          "defaultCase": [],
          "forkTasks": [],
          "startDelay": 0,
          "joinOn": [],
          "optional": false,
          "defaultExclusiveJoinTask": [],
          "asyncComplete": false,
          "loopOver": [],
          "onStateChange": {},
          "permissive": false
        },
        {
          "name": "dynamic_index",
          "taskReferenceName": "dynamic_index_ref",
          "inputParameters": {
            "forkTaskName": "LLM_INDEX_TEXT",
            "forkTaskInputs": "${transform-to-dynamic_ref.output.result}"
          },
          "type": "FORK_JOIN_DYNAMIC",
          "decisionCases": {},
          "dynamicForkTasksParam": "dynamicTasks",
          "dynamicForkTasksInputParamName": "dynamicTasksInput",
          "defaultCase": [],
          "forkTasks": [],
          "startDelay": 0,
          "joinOn": [],
          "optional": false,
          "defaultExclusiveJoinTask": [],
          "asyncComplete": false,
          "loopOver": [],
          "onStateChange": {},
          "permissive": false
        },
        {
          "name": "dynamic_index_join",
          "taskReferenceName": "dynamic_index_join_ref",
          "inputParameters": {},
          "type": "JOIN",
          "decisionCases": {},
          "defaultCase": [],
          "forkTasks": [],
          "startDelay": 0,
          "joinOn": [],
          "optional": false,
          "defaultExclusiveJoinTask": [],
          "asyncComplete": false,
          "loopOver": [],
          "onStateChange": {},
          "permissive": false
        }
      ],
      "evaluatorType": "graaljs",
      "onStateChange": {},
      "permissive": false
    }
  ],
